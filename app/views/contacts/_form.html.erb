<%= form_for(@contact) do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br>
  Address: <br>
  <%= f.label :street %>:
  <%= f.text_field :street %><br>
  <%= f.label :city %>:
  <%= f.text_field :city %><br>
  <%= f.label :state %> (abbreviation):
  <%= f.text_field :state %><br>
  <%= f.label :email %>:
  <%= f.text_field :email %><br>
  <%= f.label :plus_one_name %>:
  <%= f.text_field :plus_one_name %><br>

  <%= f.submit %>
<% end %>
