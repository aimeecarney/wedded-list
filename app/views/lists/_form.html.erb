<%= form_for(@list) do |f| %>
  <%= f.label :name %> of event:
  <%= f.text_field :name %><br>
  <%= f.label :description %>:
  <%= f.text_field :description %><br>
  <div class="field_with_errors">
  <%= f.label :date %> of event (MM/DD/YYYY):
  <%= f.text_field :date %><br>
</div>
  Address:<br>
  <%= f.label :street %>
  <%= f.text_field :street %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %> (abbreviation caps):
  <%= f.text_field :state %><br>
  <%= f.submit %>

<% end %>
